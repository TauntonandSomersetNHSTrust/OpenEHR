definition
	EVALUATION[at0000] matches {    -- Critical Care Pre Referral Information
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] matches {    -- Assessment Date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					CLUSTER[at0003] occurrences matches {0..1} matches {    -- Patient Capacity
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0004] matches {    -- Exertional Capacity
								value matches {
									1|[local::at0022],
									2|[local::at0023],
									3|[local::at0024],
									4|[local::at0025],
									5|[local::at0026],
									6|[local::at0027],
									7|[local::at0028]
								}
							}
							ELEMENT[at0005] occurrences matches {0..*} matches {    -- Exertional Limitations
								value matches {
									1|[local::at0029],
									2|[local::at0030],
									3|[local::at0031],
									4|[local::at0032],
									5|[local::at0033],
									6|[local::at0034]
								}
							}
							ELEMENT[at0006] matches {    -- Cognitive Function
								value matches {
									1|[local::at0035],
									2|[local::at0036],
									3|[local::at0037],
									4|[local::at0038]
								}
							}
							ELEMENT[at0007] matches {    -- Assistance at Home
								value matches {
									1|[local::at0039],
									2|[local::at0040],
									3|[local::at0041],
									4|[local::at0042],
									5|[local::at0043]
								}
							}
							ELEMENT[at0008] occurrences matches {0..1} matches {    -- Health and Functional Trajectory over last 12 months
								value matches {
									1|[local::at0044],
									2|[local::at0045],
									3|[local::at0046],
									4|[local::at0047]
								}
							}
						}
					}
					CLUSTER[at0009] occurrences matches {0..1} matches {    -- Frailty
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0010] matches {    -- Frailty Score
								value matches {
									1|[local::at0048],
									2|[local::at0049],
									3|[local::at0050],
									4|[local::at0051],
									5|[local::at0052],
									6|[local::at0053],
									7|[local::at0054],
									8|[local::at0055],
									9|[local::at0056]
								}
