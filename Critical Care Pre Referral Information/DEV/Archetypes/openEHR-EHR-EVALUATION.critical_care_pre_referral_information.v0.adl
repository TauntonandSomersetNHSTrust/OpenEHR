											4|[local::at0107],
											5|[local::at0108],
											6|[local::at0109]
										}
									}
								}
							}
							CLUSTER[at0075] occurrences matches {0..1} matches {    -- Liver Disease
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0079] occurrences matches {0..1} matches {    -- Alcohol Related
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
									ELEMENT[at0110] occurrences matches {0..1} matches {    -- Child-Pugh score
										value matches {
											DV_COUNT matches {*}
										}
									}
									ELEMENT[at0111] occurrences matches {0..1} matches {    -- MELD score
										value matches {
											DV_COUNT matches {*}
										}
									}
								}
							}
							CLUSTER[at0077] occurrences matches {0..1} matches {    -- Malignancy
