								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0014] occurrences matches {0..1} matches {    -- Patient wishes and expectations
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0015] occurrences matches {0..1} matches {    -- Willing to be in hospital for long perod?
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {    -- Willing to be less independent?
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
